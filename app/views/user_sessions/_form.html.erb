<div id="animated-example" class="animated fadeInDownBig reg-form">
  <%= form_tag user_sessions_path, :method => :post do %>
  <p>  <%= text_field_tag :email, nil , placeholder: "Username" %></p>
  <p>  <%= password_field_tag :password %> </p>
    <%= submit_tag "Login", class: 'button success round' %>
  <% end %>
</div>